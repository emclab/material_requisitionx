<% if @view_in_config && @js_erb_code %>
  <%= render inline: ERB.new(@js_erb_code).result(binding) %>
  
<% else %>
<% if params[:requisition][:field_changed] == 'item_autocomplete' && @digi_keys.present? %>
  <% @auto_strings.each_with_index do |v, i| %>
    <% item = MaterialRequisitionx.item_class.find_by_name(v)%>
    <% if item %>
      $('#requisition_material_items_attributes_' + <%=@digi_keys[i]%> + '_item_id').val("<%=item.id %>");
      $('#requisition_material_items_attributes_' + <%=@digi_keys[i]%> + '_spec').val('<%=item.spec%>');
      $('#requisition_material_items_attributes_' + <%=@digi_keys[i]%> + '_unit').val('<%=item.unit%>');
    <% else %>
      $('#requisition_material_items_attributes_' + <%=@digi_keys[i]%> + '_item_id').val('');
      $('#requisition_material_items_attributes_' + <%=@digi_keys[i]%> + '_spec').val('');
      $('#requisition_material_items_attributes_' + <%=@digi_keys[i]%> + '_unit').val('');
    <% end %> 
  <% end %>
  $('#requisition_field_changed').val('');
<% end %>

<% end %>
